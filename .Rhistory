customize.ggplot.axis = function(p){
p = p + labs(x = "X", y = "Y") +
scale_y_continuous(breaks = seq(from = 2000, to = 14000, by = 4000)) +
coord_cartesian(xlim = c(22, 61), ylim = c(2000, 14000))
return(p)
}
